<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.workit">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".activity.AddEtapeActivity">
        </activity>

        <activity android:name=".activity.AddWorkoutActivity">
        </activity>

        <activity android:name=".activity.EditEtapeActivity">
        </activity>

        <activity android:name=".activity.ShowTrainingActivity">
        </activity>

        <activity android:name=".activity.ShowWorkActivity">
        </activity>
    </application>

</manifest>